<%- include('header-part') %>
<%- include ('navbar') %>
<body>
  <h1 style="font-size: 70px; padding-top: 4%; text-align: center;">WELCOME TO HACKTIV SCHOOL</h1><br>
  <h3 style="text-align: center;">Here is Our music class</h3><br><br>

  <div class="row">
    <div class="col-sm"></div>
    <div class="col-sm">
      <table class="table">
        <thead>
          <tr>
            <th>Class</th>
            <th>Class Members</th>
            <th>Limit</th>
            <th>Description</th>
            <th>Action</th>
          </tr>
        </thead>
    
        <tbody>
          <% data.forEach(el => { %>
          <tr>
            <td><%= el.tipe %></td>
            <td>2</td>
            <td><%= el.limit %></td>
            <td><%= el.description %></td>
            <td>
              <a href="/<%=el.tipe%>/rules">Join</a>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
    <div class="col-sm"></div>
  </div>

</body>
</html>